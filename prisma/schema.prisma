generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Project {
  id            String   @id @default(uuid())
  title         String   @db.VarChar(255)
  description   String
  image         String?
  sourceCodeUrl String?
  hostedUrl     String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
